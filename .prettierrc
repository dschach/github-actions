{
	"trailingComma": "none",
	"singleQuote": true,
	"tabWidth": 2,
	"printWidth": 150,
	"useTabs": true,
	"bracketSpacing": true,
	"bracketSameLine": true,
	"overrides": [
		{
			"files": "*.{yml,yaml}",
			"options": { "tabWidth": 2, "useTabs": false, "singleQuote": true }
		},
		{
			"files": ".prettierrc",
			"options": { "parser": "json" }
		}
	]
}
